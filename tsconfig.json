
{
  "compilerOptions": {
    "target": "ES2020", 
    "module": "CommonJS", 
    "outDir": "./dist", 
    "strict": true,
    "esModuleInterop": true, 
    "skipLibCheck": true, 
    "forceConsistentCasingInFileNames": true, 
    "declaration": false, 
    "moduleResolution": "node",
    "jsx": "react", 
    "rootDir": "./src", 
    "baseUrl": "./", 
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "paths": {
      
    },
    "typeRoots": ["./node_modules/@types"] 
  },
  "include": ["src/**/*.ts"], 
  "exclude": ["node_modules"]
}
